<h3><%= @order_of_phoenix_members.count %> Results</h3>
<% @order_of_phoenix_members.each do |member| %>
  <ul class="member_of_order_of_the_phoenix">
    <li class="name"><%= member.name %></li>
    <li class="role"><%= member.role %></li>
    <li class="house"><%= member.house %></li>
    <li class="patronus"><%= member.patronus %></li>
  </ul>
<% end %>
